{
  "name": "r45",
  "version": "0.0.1",
  "description": "...",
  "scripts": {
    "build:client": "node_modules/.bin/rollup -c rollup.client.config.js",
    "build:server": "node_modules/.bin/rollup -c rollup.server.config.js",
    "build:all": "yarn run build:client && yarn run build:server",
    "watch:client": "node_modules/.bin/nodemon --watch src -e js,css --exec \"yarn run build:client\"",
    "watch:server": "node_modules/.bin/nodemon --watch src/server --watch src/model --watch src/action-handlers -e js --exec \"yarn run build:server && node server.bundle.js \"",
    "start": "yarn run build:all & node server.bundle.js",
    "debug": "node --inspect server.bundle.js",
    "lint": "eslint src",
    "test": "find ./src -name '*.spec.js' | xargs mocha  --compilers js:babel-core/register --require babelhook --reporter spec"
  },
  "engines": {
    "node": "7.3.0"
  },
  "keywords": [],
  "author": "Krzysztof Kabat (https://github.com/Odrinwhite)",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "*",
    "modular-css": "^0.27.0",
    "nodemon": "^1.10.2",
    "rimraf": "^2.4.3",
    "rollup": "^0.38.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-includepaths": "^0.1.5",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-root-import": "^0.2.0"
  },
  "dependencies": {
    "express": "^4.13.4",
    "react": "latest",
    "react-dom": "latest",
    "react-redux": "^5.0.1",
    "redux": "3.6.0",
    "socket.io": "^1.7.2",
    "winston": "^2.3.0",
    "ws": "^1.1.1"
  }
}
